
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/UILayout/Index.cshtml";
}

<!DOCTYPE html>
<html lang="en">


<body data-bs-spy="scroll" data-bs-target="#header" tabindex="0">


	@await Component.InvokeAsync("_UISliderComponent")

	<section id="client-holder" data-aos="fade-up">
		<div class="container">
			<div class="row">
				<div class="inner-content">
					<div class="logo-wrap">
						<div class="grid">
							<a href="#"><img src="~/Themes/booksaw/images/client-image1.png" alt="client"></a>
							<a href="#"><img src="~/Themes/booksaw/images/client-image2.png" alt="client"></a>
							<a href="#"><img src="~/Themes/booksaw/images/client-image3.png" alt="client"></a>
							<a href="#"><img src="~/Themes/booksaw/images/client-image4.png" alt="client"></a>
							<a href="#"><img src="~/Themes/booksaw/images/client-image5.png" alt="client"></a>
						</div>
					</div><!--image-holder-->
				</div>
			</div>
		</div>
	</section>

	@await Component.InvokeAsync("_UIFeatureBooksComponent")

	@await Component.InvokeAsync("_UIBestSellingComponent")

	@await Component.InvokeAsync("_UIPopularBooksComponent")

	@await Component.InvokeAsync("_UIWordOfTheDayComponent")

	@await Component.InvokeAsync("_UISubscribeComponent")

	<script src="/Themes/booksaw/js/jquery-1.11.0.min.js"></script>
	<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js"
			integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm"
			crossorigin="anonymous"></script>
	<script src="/Themes/booksaw/js/plugins.js"></script>
	<script src="/Themes/booksaw/js/script.js"></script>


</body>

</html>

